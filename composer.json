{
  "name": "goetas-webservices/xsd2php",
  "description": "Convert XSD  (XML Schema) definitions into PHP classes and JMS metadata",
  "type": "library",
  "authors": [
    {
      "name": "Asmir Mustafic",
      "email" : "goetas@gmail.com"
    }
  ],
  "keywords": [
    "converter",
    "xml",
    "xsd",
    "php",
    "jms",
    "serializer"
  ],
  "license": "MIT",
  "require": {
    "php": "^8.0",
    "symfony/console": "^6.0",
    "symfony/dependency-injection": "^6.0",
    "symfony/yaml": "^6.0",
    "symfony/config": "^6.0",
    "goetas-webservices/xsd-reader": "^0.3.9",
    "doctrine/inflector": "^2.0",
    "laminas/laminas-code": "^4.5",
    "psr/log": "^3.0"
  },
  "autoload": {
    "psr-4": {
      "GoetasWebservices\\Xsd\\XsdToPhp\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "GoetasWebservices\\Xsd\\XsdToPhp\\Tests\\": "tests/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.4-dev"
    }
  },
  "bin": [
    "bin/xsd2php"
  ],
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "jms/serializer": "^3.17",
    "symfony/validator": "^6.0",
    "dms/phpunit-arraysubset-asserts": "^0.3.1"
  }
}
